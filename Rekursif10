package com.mycompany.rekursif;

/**
 *
 * @author Outstanding Student
 */
public class Rekursif10 {
    public static boolean gollike(int p, int q){
        if(q == 0)
            return (p == 1);
        return gollike(q, p % q);
    }
    public static void main(String[] args){
        System.out.println(gollike(1,0));
    }
} 
